search bolding {
  document test {

    # Setting attribute properties for a non-existent attribute should fail
    field batchid type int {
      indexing: summary | index
      attribute {
        prefetch
      }
    }

    # ... but this is OK
    field anotherbatchid type int {
      indexing: summary | index | attribute
      attribute {
        prefetch
      }
    }
  }
}
