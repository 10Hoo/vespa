search exactmatch {

  document exactmatch {

    field tag type string {
      indexing: summary | index
      match: exact
    }

    field screweduserids type string {
      indexing: index | summary | attribute
      match {
        exact
        exact-terminator: "*!!!*"
      }
    }

  }

}
