search tensor {
  document tensor {
    field f1 type tensor {
      indexing: summary
    }
    field f2 type tensor {
      indexing: attribute
    }
    field f3 type tensor {
      indexing: attribute | summary
    }
    field f4 type tensor {
      indexing: attribute
      attribute: tensor(x[10],y[20])
    }
  }
}
